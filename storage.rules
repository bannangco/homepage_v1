rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read;
      allow write: if request.auth != null || 
                   request.origin.matches('https://bannangco-homepage-v1.web.app') ||
                   request.origin.matches('https://bannangco-homepage-v1.firebaseapp.com');
    }
  }
} 